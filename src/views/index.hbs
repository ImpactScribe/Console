<div>
  <div id="nav">
    &#8192;&#8192;&#8192;&#8192;&#8192;&#8192;
    <span>File</span>&#8192;&#8192;&#8192;&#8192;&#8192;&#8192;
    <span>Edit</span>&#8192;&#8192;&#8192;&#8192;&#8192;&#8192;
    <span>Help</span>
    <div class="bbtn">
      Time&#8192;&#8192;&#8192;&#8192;&#8192;&#8192;
    </div>
  </div>
  <div>
    {{! {/* Apps */} }}
    <div class="nes-container subtle">
      <div>
        {{! {/* M3ters Icon */} }}
        <div>
          <div>
            <div>
              <button class="nes-btn"style="background-color: #eee">
                <div>
                  <img
                    alt="M3ter"
                    width="60"
                    height="60"
                    src="https://mqtt-node.vercel.app/api/m3ter-head/pretty"
                    class="nes-avatar"
                  />
                  <img
                    alt="M3ter"
                    width="60"
                    height="60"
                    src="https://mqtt-node.vercel.app/api/m3ter-head/new"
                    class="nes-avatar"
                  />
                </div>
                <div>
                  <img
                    alt="M3ter"
                    width="60"
                    height="60"
                    src="https://mqtt-node.vercel.app/api/m3ter-head/blob"
                    class="nes-avatar"
                  />
                  <img
                    alt="M3ter"
                    width="60"
                    height="60"
                    src="https://mqtt-node.vercel.app/api/m3ter-head/now"
                    class="nes-avatar"
                  />
                </div>
              </button>
              <p>M3ters</p>
            </div>
          </div>
        </div>

        {{! {/* Github Icon */} }}
        <div>
          <div style="float:right">
            <a class="nes-btn is-warning"target="browser">
              <i class="nes-icon heart is-medium"></i>
            </a>
            <p>Paint</p>
          </div>
        </div>

        {{! {/* Terminal Icon */} }}
        <div>
          <div>
            <button
              class="nes-btn is-success"
              style="background-color: black"
            >
              <h1 style="fontSize: 26">&gt;_</h1>
            </button>
            <p>console</p>
          </div>
        </div>

        {{! {/* Browser Icon */} }}
        <div>
          <div style="float: right">
            <button class="nes-btn">
              <i class="nes-icon google is-medium"></i>
            </button>
            <p>browse</p>
          </div>
        </div>

        {{! {/* M3ters Table */} }}
        <div
          class="nes-table-responsive nes-container hidden"
          id="m3ters"
          style="background-color: white,float:left"
        >
          <div>
            <button class="nes-btn is-error">
              <i class="nes-icon close is-small"></i>
            </button>
            <button class="nes-btn">
              _
            </button>
          </div>
          <div class="nes-field is-inline">
            <input
              type="text"
              id="tokenId_input"
              class="nes-input"
              placeholder="tokenId"
            />
            <button type="button"class="nes-btn is-warning">
              + Add M3ter
            </button>
            <label>
              <input
                type="checkbox"
                class="nes-checkbox"
                defaultChecked="false"
              />
              <span>sudo</span>
            </label>
          </div>

          <table class="nes-table is-bordered">
            <thead class="nes-container is-centered">
              <tr>
                <th>TokenId</th>
                <th>Warp Contract</th>
              </tr>
            </thead>
            <tbody>
              {{#each m3ters}}
                <tr>
                  <td>
                    <span> #{{{tokenId}}}</span>
                  </td>
                  <td>
                    <a
                      target="browser"
                      href="https://viewblock.io/arweave/contract/{{{contractId}}}"
                    >
                      {{{contractId}}}
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>

        {{! {/* console Terminal */} }}
        <div class="nes-container is-dark hidden"id="console">
          <button class="nes-btn is-error">
            <i class="nes-icon close is-small"></i>
          </button>
          <button class="nes-btn">
            _
          </button>
          <div class="nes-container is-dark"id="m3ter-data">
            <div class="nes-container is-dark is-centered subtle">
              <h3 class="empty">Loading M3ter data...</h3>
            </div>
          </div>
          <span></span>
        </div>
      </div>
    </div>

    {{! {/* google app */} }}
    <div class="nes-container subtle hidden"id="browser">
      <div class="nes-container"style="background-color:white">
        <button class="nes-btn is-error">
          <i class="nes-icon close is-small"></i>
        </button>
        <button class="nes-btn">
          _
        </button>
        <iframe
          src="https://www.wikipedia.org/"
          name="browser"
          title="Google search"
        ></iframe>
      </div>
    </div>

    <button
      type="button"
      id="scroll"
      class="nes-btn"
      style="
            display: none,
            position: fixed,
            bottom: 70,
            right: 70,
            zIndex: 99"
    >
      <span>&#x2191;</span>
    </button>
    <audio ref="clickRef"src="/rclick-13693.mp3"/>
  </div>
</div>